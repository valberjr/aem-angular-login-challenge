<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Angular - Label Component"
          sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
      <layout jcr:primaryType="nt:unstructured"
              margin="{Boolean}false"
              sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
      <items jcr:primaryType="nt:unstructured">
         <column jcr:primaryType="nt:unstructured"
                 sling:resourceType="granite/ui/components/coral/foundation/container">
            <items jcr:primaryType="nt:unstructured">
               <label cq-msm-lockable="./label"
                      fieldLabel="Label"
                      jcr:primaryType="nt:unstructured"
                      name="./label"
                      sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <className cq-msm-lockable="./className"
                          fieldLabel="Estilo"
                          jcr:primaryType="nt:unstructured"
                          name="./className"
                          sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup">
                  <items jcr:primaryType="nt:unstructured">
                     <login-main-text checked="{Boolean}false"
                                      jcr:primaryType="nt:unstructured"
                                      name="className"
                                      text="Login Main Text"
                                      value="login-main-text"/>
                     <login-sub-text checked="{Boolean}false"
                                     jcr:primaryType="nt:unstructured"
                                     name="className"
                                     text="Login Sub Text"
                                     value="login-sub-text"/>
                     <login-text checked="{Boolean}false"
                                 jcr:primaryType="nt:unstructured"
                                 name="className"
                                 text="Login Text"
                                 value="login-text"/>
                     <home-mission-text checked="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        name="className"
                                        text="Home Mission Text"
                                        value="home-mission-text"/>
                     <home-missao-text checked="{Boolean}false"
                                       jcr:primaryType="nt:unstructured"
                                       name="className"
                                       text="Home MissÃ£o Text"
                                       value="home-missao-text"/>
                     <home-to-transform-text checked="{Boolean}false"
                                             jcr:primaryType="nt:unstructured"
                                             name="className"
                                             text="Home To Transform Text"
                                             value="home-to-transform-text"/>
                     <home-transformar-text checked="{Boolean}false"
                                            jcr:primaryType="nt:unstructured"
                                            name="className"
                                            text="Home Transformar Text"
                                            value="home-transformar-text"/>
                     <home-building-text checked="{Boolean}false"
                                         jcr:primaryType="nt:unstructured"
                                         name="className"
                                         text="Home Building Text"
                                         value="home-building-text"/>
                     <home-construindo-text checked="{Boolean}false"
                                            jcr:primaryType="nt:unstructured"
                                            name="className"
                                            text="Home Construindo Text"
                                            value="home-construindo-text"/>
                     <home-that-enable-text checked="{Boolean}false"
                                            jcr:primaryType="nt:unstructured"
                                            name="className"
                                            text="Home That Enable Text"
                                            value="home-that-enable-text"/>
                     <home-que-permitam-text checked="{Boolean}false"
                                             jcr:primaryType="nt:unstructured"
                                             name="className"
                                             text="Home Que Permitem Text"
                                             value="home-que-permitam-text"/>
                     <home-footer-text checked="{Boolean}false"
                                       jcr:primaryType="nt:unstructured"
                                       name="className"
                                       text="Home Footer Text"
                                       value="home-footer-text"/>
                  </items>
               </className>
            </items>
         </column>
      </items>
   </content>
</jcr:root>
