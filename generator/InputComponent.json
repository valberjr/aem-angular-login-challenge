                        }
                    },
                    {
                        "field": "password",
                        "type": "radio",
                        "attributes": {
                            "checked": "{Boolean}false",
                            "name": "type",
                            "text": "password",
                            "value": "password"
                        }
                    }
                ]
            },
            {
                "field": "placeholder",
                "label": "placeholder",
                "type": "textfield",
                "json-expose": true,
                "json-property": "placeholder"
            }
        ]
    }
